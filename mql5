// Main function
int OnInit()
{
    // Call necessary functions for the required features and requirements
    PriceActionScreener();
    CalculatePivotPoints('Daily');
    CalculatePivotPoints('Weekly');
    CalculatePivotPoints('Monthly');
    PullbackDetection();
    HarmonicPatternAndVolumeAnalysis();
    BankLevelZones();
    AllInOneLevelIndicator();
    
    return(INIT_SUCCEEDED);
}

// Start the program
void OnStart()
{
    // Start the program by calling the OnInit function
    OnInit();
}
